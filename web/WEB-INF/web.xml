<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>ListApplicantsServlet</servlet-name>
        <servlet-class>servlet.ListApplicantsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListApplicantsServlet</servlet-name>
        <url-pattern>/list</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddApplicantServlet</servlet-name>
        <servlet-class>servlet.AddApplicantServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddApplicantServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FindByNameServlet</servlet-name>
        <servlet-class>servlet.FindByNameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindByNameServlet</servlet-name>
        <url-pattern>/findByName</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FindByIdServlet</servlet-name>
        <servlet-class>servlet.FindByIdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindByIdServlet</servlet-name>
        <url-pattern>/findById</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FindExcelentApplicantsServlet</servlet-name>
        <servlet-class>servlet.FindExcelentApplicantsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindExcelentApplicantsServlet</servlet-name>
        <url-pattern>/findExcelentApplicants</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ApplicantsWithRuralCoefServlet</servlet-name>
        <servlet-class>servlet.ApplicantsWithRuralCoefServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ApplicantsWithRuralCoefServlet</servlet-name>
        <url-pattern>/ruralCoef</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FindByEnteredNameAndResultServlet</servlet-name>
        <servlet-class>servlet.FindByEnteredNameAndResultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindByEnteredNameAndResultServlet</servlet-name>
        <url-pattern>/findByNameAndResult</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateApplicantServlet</servlet-name>
        <servlet-class>servlet.UpdateApplicantServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateApplicantServlet</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteApplicantServelt</servlet-name>
        <servlet-class>servlet.DeleteApplicantServelt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteApplicantServelt</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
</web-app>